generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Favorite {
  type            String
  id              String
  title           String
  novelupdated_at String?
  page            Int
  read            Int     @default(0)

  @@id([type, id])
  @@map("favorites")
}

model Page {
  type    String
  id      String
  num     Int
  page_id String
  title   String

  @@id([type, id, num])
  @@map("pages")
}
